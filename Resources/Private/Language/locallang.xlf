<?xml version="1.0" encoding="UTF-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="messages" date="2024-01-01T00:00:00Z">
        <header/>
        <body>
            <trans-unit id="extension.title" resname="extension.title">
                <source>LLMs.txt Generator</source>
            </trans-unit>
            <trans-unit id="extension.description" resname="extension.description">
                <source>Generates compact llms.txt files for AI/LLM crawlers with page overview and access documentation</source>
            </trans-unit>
            <trans-unit id="pageStructure" resname="pageStructure">
                <source>Page Structure</source>
            </trans-unit>
            <trans-unit id="settings.intro" resname="settings.intro">
                <source>Website description for llms.txt intro section</source>
            </trans-unit>
            <trans-unit id="settings.excludePages" resname="settings.excludePages">
                <source>Comma-separated page UIDs to exclude from llms.txt</source>
            </trans-unit>
            <trans-unit id="settings.includeHidden" resname="settings.includeHidden">
                <source>Include hidden pages in llms.txt</source>
            </trans-unit>
            <trans-unit id="settings.apiKey" resname="settings.apiKey">
                <source>API Key for protected access to ?format=clean/md endpoints</source>
            </trans-unit>
            <trans-unit id="settings.apiKey.description" resname="settings.apiKey.description">
                <source>If set, the ?format=clean and ?format=md endpoints require this API key. Pass via X-LLM-API-Key header or api_key query parameter. Leave empty for public access.</source>
            </trans-unit>
            <trans-unit id="notification.robotsTxt.title" resname="notification.robotsTxt.title">
                <source>LLMs.txt: robots.txt configuration</source>
            </trans-unit>
            <trans-unit id="notification.robotsTxt.message" resname="notification.robotsTxt.message">
                <source>The llms_txt extension recommends adding llms.txt references to your robots.txt file. See the extension documentation for details.</source>
            </trans-unit>
            <trans-unit id="pages.tab.llm" resname="pages.tab.llm">
                <source>LLM</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_description" resname="pages.tx_llmstxt_description">
                <source>LLM Description</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_description.description" resname="pages.tx_llmstxt_description.description">
                <source>Special description for AI/LLM crawlers. If empty, the standard meta description is used.</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_summary" resname="pages.tx_llmstxt_summary">
                <source>LLM Summary</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_summary.description" resname="pages.tx_llmstxt_summary.description">
                <source>Additional summary text for this page. Displayed as a quote block in llms.txt.</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_keywords" resname="pages.tx_llmstxt_keywords">
                <source>LLM Keywords</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_keywords.description" resname="pages.tx_llmstxt_keywords.description">
                <source>Comma-separated keywords/topics for this page, helping LLMs understand the content.</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_exclude" resname="pages.tx_llmstxt_exclude">
                <source>Exclude from llms.txt</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_exclude.description" resname="pages.tx_llmstxt_exclude.description">
                <source>If enabled, this page will not be included in the llms.txt file.</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_priority" resname="pages.tx_llmstxt_priority">
                <source>LLM Priority</source>
            </trans-unit>
            <trans-unit id="pages.tx_llmstxt_priority.description" resname="pages.tx_llmstxt_priority.description">
                <source>Priority for this page (0-100). Higher values appear first in llms.txt. Default is 0 (normal sorting by page tree).</source>
            </trans-unit>
        </body>
    </file>
</xliff>
